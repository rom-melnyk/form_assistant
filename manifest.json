{
  "manifest_version": 2,
  "name": "Form Assistant",
  "version": "0.1.0",

  "description": "Helps with filling in standard forms",
  "icons": {
    "16": "gfx/icon16.png",
    "48": "gfx/icon48.png",
    "128": "gfx/icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "gfx/icon19.png",
      "38": "gfx/icon38.png"
    },
    "default_title": "Form Assistant",
    "default_popup": "popup.html"
  },

  "author": "Roman Melnyk <email.rom.melnyk@gmail.com>",
  "current_locale": "en",
  "homepage_url": "https://github.com/rom-melnyk/form_assistant",
  "incognito": "spanning",
  "offline_enabled": true,
  "optional_permissions": ["tabs"],
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "tabs"
  ],
  "short_name": "Form Assistant"
}

